@model List<RealEstateBusiness.Models.Staff>
@using RealEstateBusiness.Models;

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>StaffInBranch</title>
    <style>
        div {
            font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
            color: cadetblue;
        }
    </style>
</head>
<body>
    <div>
        <h1>Staffs Names in Position</h1>
        @foreach (Staff staff in Model)
        {
            @staff.FName
            @staff.LName
            <br />
        }
    </div>
</body>
</html>
